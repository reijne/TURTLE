div.main b { color: #b14a27;}

div.main li { margin-bottom: 1em; }

div.main li { margin-top: 1em; }

div.main ol { list-style: decimal;}

/* Stop the first letter being highlighted if we're in a list */
div.main ol > p:first-letter { font-size: 100%; color: #3e5b8e;}
div.main ul > p:first-letter { font-size: 100%; color: #3e5b8e;}

/* Make code look _cool_ */
div.main tt { color: #000000;}
div.main kbd { color: #000000;}


/* Header Stuff */
