#!/bin/csh
setenv ed3 mfged3
setenv ed4 mfged4
../../bin/gamess << EOF
TIME 45
CORE 1000000
TITLE        
C6H5.NO2  3-21G DIIS INFORMATION TO ED4
NOPRINT
ZMAT ANGSTROM
C
N 1 RCN
X 2 1.0 1 90.0
C 1 RCC1 2 T1 3 P1
C 1 RCC1 2 T1 3 -P1
C 4 RCC2 1 T2 2 P2
C 5 RCC2 1 T2 2 P2
C 7 RCC3 5 T3 1 P3
O 2 RNO1 1 T5 3 -90.0
O 2 RNO1 1 T5 3  90.0
H 4 RCH1 1 T6 2 P5
H 5 RCH1 1 T6 2 P5
H 6 RCH2 4 T7 11 P6
H 7 RCH2 5 T7 12 P6
H 8 RCH3 7 T8 14 P7
VARIABLES
RCN 1.49\RCC1 1.37\RCC2 1.43\RCC3 1.37
RNO1 1.21\RCH1 1.084\RCH2 1.084\RCH3 1.084
T1 120.0\T2 120.0\ T3 120.0\T5 120.0\T6 120.0
T7 120.0\T8 120.0\P1 90.0\P2 180.0\P3 0.0
P5 0.0\P6 0.0\P7 0.0\END
SCFTYPE DIRECT RHF
DIIS ED4 1
MAXCYC 8
ENTER
EOF
../../bin/gamess << EOF
TIME 240
CORE 1000000
RESTART SCF
TITLE        
C6H5.NO2  3-21G  DIIS INFO TO ED4 / SCF energy  -431.6987558
NOPRINT
ZMAT ANGSTROM
C
N 1 RCN
X 2 1.0 1 90.0
C 1 RCC1 2 T1 3 P1
C 1 RCC1 2 T1 3 -P1
C 4 RCC2 1 T2 2 P2
C 5 RCC2 1 T2 2 P2
C 7 RCC3 5 T3 1 P3
O 2 RNO1 1 T5 3 -90.0
O 2 RNO1 1 T5 3  90.0
H 4 RCH1 1 T6 2 P5
H 5 RCH1 1 T6 2 P5
H 6 RCH2 4 T7 11 P6
H 7 RCH2 5 T7 12 P6
H 8 RCH3 7 T8 14 P7
VARIABLES
RCN 1.49\RCC1 1.37\RCC2 1.43\RCC3 1.37
RNO1 1.21\RCH1 1.084\RCH2 1.084\RCH3 1.084
T1 120.0\T2 120.0\ T3 120.0\T5 120.0\T6 120.0
T7 120.0\T8 120.0\P1 90.0\P2 180.0\P3 0.0
P5 0.0\P6 0.0\P7 0.0\END
SCFTYPE DIRECT RHF
DIIS ED4 1
ENTER
EOF
