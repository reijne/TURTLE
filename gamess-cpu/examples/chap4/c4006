#!/bin/csh
setenv ed2 mfged2
setenv ed3 mfged3
../../bin/gamess << EOF
TITLE
HCN T.S. - SYNCHRONOUS TRANSIT energy = -92.6250191
ZMAT ANGS
C
X 1 1.0
N 1 CN 2 90.0
H 1 CH 2 90.0 3 HCN
VARIABLES
CN 1.1484 MINIMA 1.1371 1.1597
CH 1.5960 MINIMA 1.0502 2.1429
HCN 90.0   MINIMA 180.0  0.0
END
BASIS SV 4-31G
RUNTYPE SADDLE
LSEARCH 0 4
ENTER
EOF
