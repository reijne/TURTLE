#!/bin/csh
setenv ed2 mfged2
setenv ed3 mfged3
../../bin/gamess << EOF
TITLE\CUCL 3-21G HARMONIC CI(1M) energy -2088.28516166
HARMONIC
ZMAT ANGSTROM\CU\CL 1 CUCL
VARIABLES\CUCL 2.093 \END 
BASIS 3-21G
RUNTYPE CI
CORE
1 TO 14 END
ACTIVE
15 TO 40 END
DIRECT 18 9 17
CONF
2 2 2 2 2 2 2 2 2 
ENTER
EOF
../../bin/gamess << EOF
RESTART NEW
TITLE\CUCL 3-21G HARMONIC CI(2M) energy -2088.29739535
HARMONIC
BYPASS SCF
ZMAT ANGSTROM\CU\CL 1 CUCL
VARIABLES\CUCL 2.093 \END 
BASIS 3-21G
RUNTYPE CI
CORE
1 TO 14 END
ACTIVE
15 TO 40 END
DIRECT 18 10 16
CONF
2 2 2 2 2 2 2 2 2 0
2 2 2 2 2 2 2 2 0 2
ENTER
EOF
