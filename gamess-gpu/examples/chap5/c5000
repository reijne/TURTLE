#!/bin/csh
setenv ed2 mfged2
setenv ed3 mfged3
setenv ed4 mfged4
setenv ed6 mfged6
../../bin/gamess << EOF
TIME 3
TITLE
H2CO - TZVP BASIS - TRANSFORMATION/ SCF energy  -113.908210
SUPER OFF NOSYM
ZMATRIX ANGSTROM
C
O 1 1.203
H 1 1.099 2 121.8
H 1 1.099 2 121.8 3 180.0
END
BASIS TZVP
RUNTYP TRANSFORM
PASS 2 2
TRACC 9
ENTER
EOF
../../bin/gamess << EOF
RESTART TRANSFORM
TITLE
H2CO - TZVP BASIS - TRANSFORMATION
SUPER OFF NOSYM
ZMATRIX ANGSTROM
C
O 1 1.203
H 1 1.099 2 121.8
H 1 1.099 2 121.8 3 180.0
END
BASIS TZVP
RUNTYP TRANSFORM
PASS 2 2
TRACC 9
ENTER
EOF
