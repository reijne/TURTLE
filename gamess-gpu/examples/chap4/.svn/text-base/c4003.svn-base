#!/bin/csh
setenv ed2 mfged2
setenv ed3 mfged3
../../bin/gamess << EOF
TITLE\H2O DZ OPTIMIZATION STARTUP energy = -76.0109982
ZMAT ANGSTROM\O\H 1 ROH\H 1 ROH 2 THETA                       
VARIABLES\ROH 0.956 HESSIAN 0.7\THETA 104.5 HESSIAN 0.2 \END        
BASIS DZ
RUNTYPE OPTXYZ\MINMAX 1 1
ENTER
EOF
../../bin/gamess << EOF
RESTART OPTXYZ
TITLE\H2O DZ OPTIMIZE energy  = -76.01100163 hartree
ZMAT ANGSTROM\O\H 1 ROH\H 1 ROH 2 THETA                       
VARIABLES\ROH 0.956 HESSIAN 0.7\THETA 104.5 HESSIAN 0.2 \END        
BASIS DZ
RUNTYPE OPTXYZ
ENTER
EOF
