#!/bin/csh
setenv ed2 mfged2
setenv ed3 mfged3
../../bin/gamess << EOF
TITLE
H2CO - DZP BASIS - 
ZMATRIX ANGSTROM
C
O 1 1.203
H 1 1.099 2 121.8
H 1 1.099 2 121.8 3 180.0
END
BASIS DZP
ENTER
EOF
../../bin/gamess << EOF
RESTART NEW
TITLE
H2CO - DZP BASIS - BOYS VALENCE LOCALISATION
ZMATRIX ANGSTROM
C
O 1 1.203
H 1 1.099 2 121.8
H 1 1.099 2 121.8 3 180.0
END
BASIS DZP
RUNTYPE ANALYSE
LOCAL BOYS
3 TO 8 END
VECTORS 1
ENTER 2
EOF
../../bin/gamess << EOF
RESTART NEW
TITLE
H2CO - DZP BASIS - FOSTER BOYS VALENCE LOCALISATION
ZMATRIX ANGSTROM
C
O 1 1.203
H 1 1.099 2 121.8
H 1 1.099 2 121.8 3 180.0
END
BASIS DZP
RUNTYPE ANALYSE
LOCAL BOYS DEFAULT
VECTORS 1
ENTER 2
EOF
../../bin/gamess << EOF
RESTART NEW
TITLE
H2CO - DZP BASIS -  PIPEK-MEZEY VALENCE LOCALISATION
ZMATRIX ANGSTROM
C
O 1 1.203
H 1 1.099 2 121.8
H 1 1.099 2 121.8 3 180.0
END
